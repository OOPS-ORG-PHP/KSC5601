	</version>
	<stability>
		<release>stable</release>
		<api>stable</api>
	</stability>
	<license uri="http://www.opensource.org/licenses/bsd-license.php">BSD</license>
	<notes>* release 1.0.0</notes>
	<contents>
		<dir name="/">
			<file role="php" md5sum="@KSC5601/Stream.php@" name="KSC5601/Stream.php" />
			<file role="php" md5sum="@KSC5601/UCS2.php@" name="KSC5601/UCS2.php" />
			<file role="php" md5sum="@KSC5601/UTF8.php@" name="KSC5601/UTF8.php" />
			<file role="php" md5sum="@KSC5601/ksc5601.php@" name="KSC5601/ksc5601.php" />
			<file role="php" md5sum="@KSC5601.php@" name="KSC5601.php" />
		</dir>
	</contents>
	<dependencies>
		<required>
			<php>
				<min>5.0.0</min>
			</php>
			<pearinstaller>
				<min>1.4.3</min>
			</pearinstaller>
		</required>
		<optional>
			<package>
				<name>iconv</name>
				<channel>php</channel>
				<min>5.0.0</min>
			</package>
			<package>
				<name>mbstring</name>
				<channel>php</channel>
				<min>5.0.0</min>
			</package>
		</optional>
	</dependencies>
